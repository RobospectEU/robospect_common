<!-- Hokuyo Link Attached to the PA10 -->
<link name="hokuyo_link">
  <visual>
    <geometry>
      <mesh filename="package://pa10_description/urdf/meshes/hokuyo.dae" scale="1 1 1" />
    </geometry>
  </visual>
  <inertial>
      <mass value="0.0001" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.0001" ixy="0.0" ixz="0.0"
               iyy="0.0001" iyz="0.0" 
               izz="0.0001" />
  </inertial>
</link>

<!-- Hokuyo joint (TF) -->
  <joint name="hokuyo_joint" type="fixed">
    <parent link="link5" />
    <child link="hokuyo_link" />
    <origin xyz="0.0 0.112 0.43" rpy="-1.570796 -1.570796 0.0" />
  </joint>

<!-- Hokuyo sensor URDF-->
  <!--sensor name="hokuyo_sensor" update_rate="20">
   <parent link="hokuyo_link"/>
   <origin xyz="0 0 0" rpy="0 0 0"/>
   <ray>
     <horizontal samples="720" resolution="1" min_angle="-1.5708" max_angle="1.5708"/>
     <vertical samples="1" resolution="1" min_angle="0" max_angle="0"/>
   </ray>
  </sensor-->

<!-- Gazebo plugin for Hokuyo sensor -->
  <gazebo reference="hokuyo_link">
    <sensor type="ray" name="hokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <!--visualize>false</visualize-->
      <visualize>true</visualize>
      <update_rate>40.0</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>720</samples>
            <resolution>1</resolution>
            <min_angle>-1.570796</min_angle>
            <max_angle>1.570796</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.10</min>
          <max>30.0</max>
          <resolution>0.01</resolution>
        </range>
        <noise>
          <type>gaussian</type>
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
          <mean>0.0</mean>
          <stddev>0.01</stddev>
        </noise>
      </ray>
      <plugin name="gazebo_ros_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>laser_scan</topicName>
        <frameName>hokuyo_link</frameName>
      </plugin>
    </sensor>
  </gazebo>


